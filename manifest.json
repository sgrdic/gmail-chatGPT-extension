{
  "manifest_version": 3,
  "name": "Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "main.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://mail.google.com/mail/*"
      ]
    }
  ]
}